generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Post {
  id              Int         @id @default(autoincrement())
  createdAt       DateTime    @default(now())
  fullName        String
  message         String
  amount          Int
}

model User {
id               Int         @id @default(autoincrement())
firstName        String
lastName         String
email            String       @unique
password         String     
createdAt        DateTime    @default(now())


}